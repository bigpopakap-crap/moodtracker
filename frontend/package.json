{
  "name": "@moodtracker/frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/registerServiceWorker.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.17",
    "@types/jest": "^24.0.9",
    "@types/moodtracker": "link:../types",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "axios-mock-adapter": "^1.16.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "jest-localstorage-mock": "^2.4.0",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:watch": "react-scripts test --coverage --watchAll",
    "check-deps": "yarn check --integrity && yarn check --verify-tree",
    "hard-reset": "yarn cache clean && rm -rf ./node_modules/"
  }
}
